pod 'Firebase/Database'
pod 'Firebase/RemoteConfig'

  target 'FirebasetestTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FirebasetestUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
